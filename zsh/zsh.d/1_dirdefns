# directory definitions used in aliases etc.
# ------------------------------------------------------------------------------

DIR_CODE=~/code
    DIR_MRG=$DIR_CODE/mrg
    DIR_MVL=$DIR_MRG/Thirdparty/mvl
    DIR_GLC=$DIR_CODE/glconsole
    DIR_MOOS=$DIR_CODE/moos
    DIR_MRGBASE=$DIR_CODE/mrgBase
    DIR_MRGSIM=$DIR_CODE/simulator3d
    DIR_THESIS_SVN=$DIR_CODE/rjsThesis

DIR_PAPERS=$DIR_CODE/mrgPapers

DIR_DROPBOX=~/Dropbox
    DIR_DPHIL=$DIR_DROPBOX/DPhil
        DIR_THESIS_BAK=$DIR_DPHIL/ThesisBackup

DIR_MY_BLD=~/build
    DIR_MY_BIN=$DIR_MY_BLD/bin
    DIR_MY_LIB=$DIR_MY_BLD/lib

FILE_ALIASES=$HOME/.zsh/zsh.d/2_aliases

